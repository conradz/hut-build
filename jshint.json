{
  "config": {
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "nonew": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxparams": 3,
    "maxdepth": 2,
    "maxlen": 80,
    "globalstrict": true,
    "laxbreak": true,
    "sub": true
  },

  "globals": {
    "require": false,
    "process": false,
    "window": false,
    "module": false
  }
}
